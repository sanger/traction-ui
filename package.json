{
  "name": "traction-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p serve:css serve",
    "serve": "vue-cli-service serve",
    "build": "npm-run-all build:css build:js",
    "build-js": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint --max-warnings 3",
    "pretty": "prettier --check '**/*'",
    "test": "vue-cli-service test:unit && vue-cli-service test:e2e",
    "serve:css": "postcss node_modules/@sanger/ui-styling/tailwind.css -o src/styles/index.css",
    "build:css": "postcss node_modules/@sanger/ui-styling/tailwind.css -o src/styles/index.css --env production"
  },
  "dependencies": {
    "@sanger/ui-styling": "^1.0.0",
    "autoprefixer": "^9",
    "axios": "^0.21.4",
    "bootstrap-vue": "^2.22.0",
    "core-js": "^3.22.5",
    "csv-parse": "^5.0.4",
    "flush-promises": "^1.0.2",
    "global": "^4.4.0",
    "https-proxy-agent": "^2.2.2",
    "lodash-es": "^4.17.21",
    "postcss": "^7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vue": "^2.6.12",
    "vue-router": "^3.5.3",
    "vue-selecto": "^1.16.2",
    "vue-simple-spinner": "^1.2.10",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.16",
    "@vue/cli-plugin-e2e-cypress": "~4.5.16",
    "@vue/cli-plugin-eslint": "~4.5.16",
    "@vue/cli-plugin-unit-jest": "~4.5.16",
    "@vue/cli-service": "4.5.15",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^5.2.3",
    "json-server": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7",
    "prettier": "2.5.1",
    "sass": "^1.51.0",
    "sass-loader": "^10.2.1",
    "vue-template-compiler": "^2.6.12"
  },
  "resolutions": {
    "cypress": "^7.3.0"
  }
}
