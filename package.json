{
  "name": "traction-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm-run-all -p serve:css serve",
    "serve": "vite",
    "build": "npm-run-all build:css build:js",
    "build:js": "vite build",
    "test:unit": "vitest run",
    "test:e2e": "test:e2e:all",
    "test:e2e:all": "npm-run-all -p start test:cypress:all",
    "test:e2e:one": "npm-run-all -p start test:cypress:one",
    "test:cypress:all": "cypress run",
    "test:cypress:one": "cypress open",
    "lint": "eslint . --ext .js,.vue,.cjs",
    "pretty": "prettier --check '**/*'",
    "test": "npm-run-all test:unit test:e2e:all",
    "serve:css": "postcss src/styles/tailwind.css -o src/styles/index.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/index.css --env production"
  },
  "dependencies": {
    "@sanger/ui-styling": "^4.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "csv-parse": "^5.5.5",
    "global": "^4.4.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "postcss": "^8.4.37",
    "swrv": "^1.0.4",
    "tailwindcss": "3.4.1",
    "vite": "^5.1.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue3-selecto": "^1.12.3",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@vue/test-utils": "^2.4.5",
    "cypress": "^13.7.0",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "prettier": "3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "vitest": "^1.4.0",
    "vue-template-compiler": "^2.7.5"
  }
}
