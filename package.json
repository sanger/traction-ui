{
  "name": "traction-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm-run-all -p serve:css serve",
    "serve": "vite",
    "build": "npm-run-all build:css build:js",
    "build:js": "vite build",
    "test:unit": "vitest run",
    "test:e2e": "test:e2e:all",
    "test:e2e:all": "npm-run-all -p start test:cypress:all",
    "test:e2e:one": "npm-run-all -p start test:cypress:one",
    "test:cypress:all": "cypress run",
    "test:cypress:one": "cypress open",
    "lint": "eslint . --ext .js,.vue,.cjs",
    "pretty": "prettier --check '**/*'",
    "test": "npm-run-all test:unit test:e2e:all",
    "serve:css": "postcss src/styles/tailwind.css -o src/styles/index.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/index.css --env production"
  },
  "dependencies": {
    "@sanger/ui-styling": "^4.1.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "axios": "^1.6.2",
    "autoprefixer": "^10.4.16",
    "csv-parse": "^5.5.2",
    "global": "^4.4.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "postcss": "^8.4.31",
    "swrv": "^1.0.4",
    "tailwindcss": "3.3.5",
    "vite": "^5.0.2",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5",
    "vue3-selecto": "^1.12.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@vue/test-utils": "^2.4.2",
    "cypress": "^13.6.0",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "prettier": "3.1.0",
    "rollup-plugin-visualizer": "^5.9.3",
    "sass": "^1.69.5",
    "vitest": "^0.34.6",
    "vue-template-compiler": "^2.7.5"
  }
}
